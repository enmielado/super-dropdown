{
    "name": "veryfine/super-dropdown",
    "description": "A custom field for the Craft CMS for building multiple and hierarchical dropdown fields from native elements or data.",
    "type": "craft-plugin",
    "version": "1.1.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "super dropdown",
        "dropdown",
        "field",
        "select"
    ],
    "support": {
        "docs": "https://veryfine.work/plugins/superdropdown/docs/",
        "issues": "https://github.com/veryfinework/super-dropdown/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "very-fine",
            "homepage": "https://github.com/veryfinework"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
          "veryfinework\\superdropdown\\": "src/"
        }
    },
    "extra": {
        "name": "Super dropdown",
        "handle": "super-dropdown",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/veryfinework/super-dropdown/master/CHANGELOG.md",
        "class": "veryfinework\\superdropdown\\SuperDropdown"
    }
}
